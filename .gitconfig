[user]
	name = Stefan Perju
	email = stefan.perju@yahoo.com
[branch]
	autosetuprebase = always
[alias]
	k = log --graph --decorate --pretty=oneline --abbrev-commit
	del = push origin --delete
	co = checkout
	br = branch
	ci = commit
	st = status
	pr = remote prune origin
	f = fetch
	p = pull -r
	pu = push
	me = merge
	ren = rebase origin/next
	rem = rebase origin/master
	red = rebase origin/dev
	rec = rebase --continue
	rea = rebase --abort
	re = rebase
	mt = mergetool
	dt = difftool
	t = tag
	rh = reset --hard HEAD
	s = stash
	sp = stash pop
	ignore = update-index --assume-unchanged
	unignore = update-index --no-assume-unchanged
	ignored = !git ls-files -v | grep "^[[:lower:]]"
[core]
	autocrlf = input
	editor = /usr/local/bin/atom -n -w
	excludesfile = /Users/stefan.perju/.gitignore
[merge]
	tool = opendiff
